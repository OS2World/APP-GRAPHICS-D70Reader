<x:xmpmeta xmlns:x='adobe:ns:meta/' x:xmptk='XMPTk 2.8'>

<rdf:RDF xmlns:rdf='http://www.w3.org/1999/02/22-rdf-syntax-ns#'
 xmlns:iX='http://ns.adobe.com/iX/1.0/'>

  <!-- Most applications will have an empty about attribute. -->
  <!-- It is recommended, however, that the about attribute  -->
  <!-- be set to the DocumentID or InstanceID.               -->

  <rdf:Description about=''
   xmlns:xmpS='http://ns.adobe.com/xap/1.0/s'
   xmlns:stDsp='http://ns.adobe.com/xap/1.0/sType/FileDisposition#'>

    <!-- The character entity &quot; is used instead of the  -->
    <!-- special character double quote since the XMP        -->
    <!-- Toolkit converts all special characters to their    -->
    <!-- equivalent character entities when serializing the  -->
    <!-- metadata.                                           -->

    <xmpS:EntityTag>W/&quot;A weak ETag&quot;</xmpS:EntityTag>

    <xmpS:FileDisposition>
      <rdf:Alt>

          <!-- This is an example of an Alt container that does not use -->
          <!-- the xml:lang attribute.  An item is typically selected   -->
          <!-- by the value of the OS property.                         -->

          <rdf:li parseType='Resource'>
            <stDsp:filename>aFile</stDsp:filename>
            <stDsp:OS>Windows</stDsp:OS>
            <stDsp:directoryPath>c:\mydir\path\</stDsp:directoryPath>
          </rdf:li>

          <rdf:li parseType='Resource'>
            <stDsp:filename>aFile</stDsp:filename>
            <stDsp:OS>MacOS</stDsp:OS>
            <stDsp:directoryPath>hd:mydir:path:</stDsp:directoryPath>
          </rdf:li>

          <rdf:li parseType='Resource'>
            <stDsp:filename>aFile</stDsp:filename>
            <stDsp:OS>Unix</stDsp:OS>
            <stDsp:directoryPath>/mydir/path/</stDsp:directoryPath>
          </rdf:li>

          <rdf:li parseType='Resource'>
            <stDsp:filename>aPage</stDsp:filename>
            <stDsp:OS>URL</stDsp:OS>
            <stDsp:directoryPath>http://www.foo.bar/</stDsp:directoryPath>
          </rdf:li>

      </rdf:Alt>
    </xmpS:FileDisposition>

    <!-- The xmpS:ResourceID property should never be set by an    -->
    <!-- authoring application.                                    -->

    <xmpS:ResourceID>unique timestamp2001-08-14T11:02:13Z</xmpS:ResourceID>

    <xmpS:Size>10000</xmpS:Size>

  </rdf:Description>

</rdf:RDF>

</x:xmpmeta>
<?xpacket end='r'?>

