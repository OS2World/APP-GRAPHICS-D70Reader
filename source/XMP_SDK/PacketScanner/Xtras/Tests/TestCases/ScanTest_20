 3 345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678
 4 bytes attribute = header + trailer + available padding +1:                                     |
 5 +++hB <?xpacket begin='' id='W5M0MpCehiHzreSzNTczkc9d' encoding='UTF-8' bytes='801'?>          |
 6                                                                              <?xpacket end='r'?>
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
         ---pB
14 length of bytes attribute is greater than eof:                                                 |
15 +++hB <?xpacket begin="" id="W5M0MpCehiHzreSzNTczkc9d" encoding="UTF-8" bytes="9999"?>         |
16                                                                              <?xpacket end="r"?>
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
                                                                                                   
